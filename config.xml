<?xml version='2.0' encoding='utf-8'?>
<widget id="com.jwcustoms.lakefront" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lakefront</name>
    <description>
        Shows locations accessible via boat at Lake of the Ozarks.
    </description>
    <author email="jeff.r.winters@gmail.com" href="lakefrontatloto.com">
        Jeff Winters
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
		<icon src="icon.png" density="ldpi" />
        <icon src="icon.png" density="mdpi" />
        <icon src="icon.png" density="hdpi" />
        <icon src="icon.png" density="xhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.0.0" />
	<icon src="icon.png" />

</widget>
